<Window x:Class="GuardianFlash.ReadReadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuardianFlash"
        mc:Ignorable="d"
        Title="ReadReadWindow" Height="405" Width="600">
    <Grid>
        <RichTextBox x:Name="richTextBox" HorizontalAlignment="Left" Height="129" Margin="10,55,0,0" VerticalAlignment="Top" Width="572" BorderThickness="1,1,1,5">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <RichTextBox x:Name="richTextBox1" HorizontalAlignment="Left" Height="129" Margin="10,184,0,0" VerticalAlignment="Top" Width="572">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
